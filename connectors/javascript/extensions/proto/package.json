{
  "name": "@playhouse/connector-proto",
  "version": "0.1.0",
  "description": "Protocol Buffers serialization extension for PlayHouse connector",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@playhouse/connector": "^0.1.0"
  },
  "dependencies": {
    "protobufjs": "^7.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "playhouse",
    "connector",
    "protobuf",
    "proto",
    "serialization"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ulalax/playhouse.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
